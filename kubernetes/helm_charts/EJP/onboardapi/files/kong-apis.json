[
{
  "name": "storySakhiQuery",
  "plugins": [
    {
      "name": "cors"
    },
    {
      "name": "jwt"
    },
    {
      "config.whitelist": [
        "ssQuery"
      ],
      "name": "acl"
    },
    {
      "config.hour": 5000,
      "config.limit_by": "credential",
      "config.policy": "local",
      "name": "rate-limiting"
    },
    {
      "config.allowed_payload_size": 1,
      "name": "request-size-limiting"
    }
  ],
  "strip_uri": true,
  "upstream_url": "http://storysakhi-service:8000/v1/query",
  "uris": "/storysakhi/v1/query"
},
